[General]
Version="@XHTMLDBG_VERSION@"
DefaultFontSize=14
DefaultFixedFontSize=14
css_lang=en
css_medium=all
css_warning=2
css_appl=@JAVA_RUNTIME@
enableHighlightBackground=true
