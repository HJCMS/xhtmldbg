enableProxy=false
sslCaCertsDatabase=@CA_BUNDLE_FILE@
icontheme=oxygen
iconthemepath=@ICON_THEME_PATH@
GeoIP_Database=@GEOIP_DATABASE_PATH@/GeoIP.dat

[HeaderDefinitions]
